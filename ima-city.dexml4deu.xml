 for title, url in items:
  if any([x.lower() in title.lower() for x in mylist]):
   title = '00' + title
  else:
   title = '99' + title
  chs.append((title, url))
 myorder = []
 for title, url in sorted(chs):
  myorder.append((title[2:], url))
 return myorder
]]></expres>
